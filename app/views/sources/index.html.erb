<div class="container">
  <table class="table table-striped table-responsive">
    <thead>
      <tr>
        <th>ファイル名</th>
        <th>最終更新日</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @sources.each do |source| %>
          <tr>
            <td>
              <span class="label label-default">prg</span>
              <%= link_to source.name, "/#{source.name}" %>
            </td>
            <td><%= source.updated_at %></td>
            <td>
              <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
            </td>
          </tr>
      <% end %>
    </tbody>
  </table>
  <button class="btn btn-primary">新規作成</button>
</div>